*
	margin 0 
	padding 0
	font-family Microsoft Yahei
.all
	.topbg
		width 100%
		height 60px
		background-color rgb(36,36,36)
		.top
			width 980px
			margin 0 auto
			.icon
				float left
				margin-top 10px
				span
					display inline-block
					font-size 18px
					width 42px
					font-weight bold
					color rgb(255,238,0)
					line-height 20px
			.difang
				float left
				padding 15px 0 0 0px
				margin-left 5px
				a
					display inline-block
					font-size 16px
					line-height 26px
					color white
					padding 0px 16px
					border 2px solid white
					border-radius 15px
					&:hover
						color rgb(255,238,0)
						border 2px solid 
					&.abg
						border 2px solid rgb(255,238,0)
						background-color rgb(255,238,0)
						color black
	.bannerbg
		width 100%
		height 460px
		background-repeat no-repeat
		background-image url(../img/01.png)
		.qiang
			position relative
			top 315px
			left -8px
			display block
			margin 0 auto
			width 230px
			height 60px
			border-color rgb(30,28,111)
			border-style solid
			border-width 2px 2px 4px 2px
			line-height 48px
			text-align center
			border-radius 20px
			background-color rgb(255,233,11)
			color rgb(231,21,58)
			font-size 30px
			
			span
				display inline-block
				font-weight bold
			i
				display inline-block
				animation yidong 1s infinite
	.green
		position relative
		top -1px
		height 130px
		background-color rgb(31,172,103)
		.huodong
			width 1000px
			margin 0 auto
			a
				margin-left 5px
				margin-top -16px
				display inline-block
				width 188px
				height 123px
				color white
				p
					margin-top 15px
					margin-left 10px
					font-weight bold
					font-size 24px
				span
					display inline-block
					margin-left 10px
					margin-top 10px
					font-size 14px	
				&.a1
					background-image url(../img/go_01.jpg)
				&.a2
					background-image url(../img/go_02.jpg)
				&.a3
					background-image url(../img/go_03.jpg)
				&.a4
					background-image url(../img/go_04.jpg)
				&.a5
					background-image url(../img/go_05.jpg)
.dd
	display block
	height 5px
	width 100%
.mboxbj
	margin-top -1px
	width 100%
	background #815AD8
	overflow hidden
	padding-bottom 50px
	.titl
		width 980px
		text-align center
		margin 0px auto
		.titl-img
			width 980px
			height 93px
			margin-top 60px
		.titl-menu
			width 100%
			border-radius 5px
			height 50px
			text-align center
			display flex
			justify-content center
			align-items center
			font-family "微软雅黑"
			font-size 20px
			font-weight blod
			line-height 50px
			color #4F1EBD
			position relative
			.mm
				height 50px
				width 100%
				.go-group
					width 49%
					display inline-block
					border 1px solid blue
					border-radius:10px 0px 0px 10px ;
					border-right transparent
					background #9369F0
				&>.go-group:not(.select-color):hover
					color #FFEE00 
					background #4F1EBD
				.free
					width 49%
					display inline-block
					border 1px solid blue
					border-radius:0px 10px 10px 0px;
					border-left transparent
					background #9369F0
				&>.free:not(.select-color):hover
					color #FFEE00 
					background #4F1EBD
				.select-color
					color #FFEE00 
					background #4F1EBD
			.titlmenu-flex
				position fixed
				width 980px
				top 0px
				right 0px
				left 0px
				margin-left auto
				margin-right auto
				z-index 10
		.titlImage1
			background-image url('../img/tit_02.jpg')	
	.canping-info
		width 1000px
		margin 0 auto	
		padding-left 28px
		// display flex
		// justify-content space-between
		// flex-wrap wrap 
		// align-content: flex-start 
		.yy
			margin-right 20px
			display inline-block
			height 280px
			width 220px	
			a
				height 280px
				width 220px	
				display block
				text-decoration none
				color #000
				background #fff
				overflow hidden
				margin 15px 0
				border 1px solid #848484
				.canpinginfo1box
					width 100%
					height 150px
					overflow hidden
					position relative
					.canpinginfo1box-image
						width 100%
						height 150px
						background url('../img/10.jpg') no-repeat
						background-size cover
						transition all 1000ms linear
					#boxshow
						position absolute
						top 0px
						left 0px
						width 100%
						height 100%
						background-color rgba(91,91,91,0.4)
						display none
						text-align center
						img
							height 100px
							width 100px
							margin-top 25px
						p
							font-size 16px
							color yellow
				&>.canpinginfo1box:hover .canpinginfo1box-image
					 transform scale(1.05)
				.titltxt
					height 20px 
					width 95%
					font-family "微软雅黑"
					font-size 18px
					font-weight blod
					line-height 20px
					margin 10px auto
					overflow hidden
					white-space nowrap
					text-overflow ellipsis
				.desc
					height 28px 
					width 95%
					font-family "微软雅黑"
					font-size 14px
					line-height 18px
					margin 0px auto
					overflow hidden
					white-space nowrap
					text-overflow ellipsis
				.price
					height 50px
					width 95%
					margin 0px auto
					position relative
					span
						display inline-block
					&>span:nth-of-type(1)
						height 20px
						font-size 18px
						font-weight bold
						line-height 20px
						color #FF3939
					&>span:nth-of-type(2)
						height 30px
						font-size 22px
						font-weight bold
						line-height 30px
						color #FF3939
					&>span:nth-of-type(3)
						height 20px
						font-size 16px
						font-weight bold
						line-height 20px
						color #FF3939
					.del-txt
						height 20px
						font-size 16px
						font-weight bold
						line-height 20px
						color #666666
						display inline-block
						position absolute
						left 0px
						top 25px
					.pr-btn
						text-align center
						width 70px
						height 40px
						line-height 40px
						display inline-block
						background #FF3939
						border-radius 5px
						font-family "微软雅黑"
						font-size 18px
						font-weight blod
						color #fff
						position absolute
						right 5px
						top 5px
						&:hover
							background #D82739
							color #FFE66C
			&>a:hover
				border 1px solid #FF8442
	.last-line
		width 100%
		height 1px
		border 1px solid #000
		background #000
		margin-top 50px
		position relative
		.more
			width 130px
			height 35px
			background #412D6D
			font-size 20px
			color #fff
			line-height 35px
			text-align center
			border-radius 35px
			position absolute
			top -18px
			left calc(50% - 75px)
